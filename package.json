{
  "private": true,
  "scripts": {
    "backend": "npx netlify-cms-proxy-server",
    "clean": "rm -fr ./node_modules/.cache",
    "content": "node content/index.js",
    "dev": "nuxt dev && tw",
    "build": "nuxt build",
    "lint:js": "eslint --ext \".js,.vue\" ",
    "serve": "node nuxt-build/server/index.mjs",
    "storybook": "start-storybook -p 6006 -s ./public,./assets",
    "sb:dev": "yarn storybook && watch",
    "build-storybook": "build-storybook",
    "tw:config": "tailwind-config-viewer -o",
    "tailwind": "npx tailwindcss -i ./assets/scss/tailwind.scss -c ./tailwind.config.js -o ./assets/css/main.css",
    "watch": "watch 'npm run tailwind' ./components ./assets/scss"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/vue3": "^6.3.12",
    "@tailwindcss/forms": "^0.3.4",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.3",
    "front-matter-markdown": "^0.4.5",
    "netlify-cms-app": "^2.15.54",
    "npm-run-all": "^4.1.5",
    "nuxt3": "latest",
    "postcss": "^8.3.11",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.43.3",
    "sass-loader": "^10.1.1",
    "tailwind-config-viewer": "^1.6.2",
    "tailwindcss": "^2.2.17",
    "vue-loader": "^16.8.1"
  },
  "dependencies": {
    "@nuxtjs/eslint-config": "^7.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "chart.js": "^3.5.1",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-mdx": "^1.16.0",
    "eslint-plugin-vue": "^7.20.0",
    "netlify-cms-proxy-server": "^1.3.21",
    "storybook": "^6.3.12"
  }
}
